{
  "engines":{
    "composer":"^0.16.0"
  },
  "name":"animaltracking-network",
  "version":"0.0.1",
  "description":"Animal Tracking network based on UK DEFRA regulations",
  "networkImage":"https://hyperledger.github.io/composer-sample-networks/packages/animaltracking-network/networkimage.svg",
  "networkImageanimated":"https://hyperledger.github.io/composer-sample-networks/packages/animaltracking-network/networkimageanimated.svg",
  "scripts":{
    "prepublish":"mkdirp ./dist && composer archive create --sourceType dir --sourceName . -a ./dist/animaltracking-network.bna",
    "pretest":"npm run lint",
    "test":"mocha -t 0 --recursive",
    "lint":"eslint .",
    "postlint":"npm run licchk",
    "licchk":"license-check",
    "postlicchk":"npm run doc",
    "doc":"jsdoc --pedantic --recurse -c jsdoc.json",
    "pkgstamp":"node ./scripts/pkgstamp.js"
  },
  "repository":{
    "type":"git",
    "url":"https://github.com/hyperledger/composer"
  },
  "author":"Hyperledger Composer",
  "license":"Apache-2.0",
  "keywords":["defra","farmer","compliance","animal","movement","composer","composer-network"],
  "devDependencies":{
    "chai":"^3.5.0",
    "composer-admin":"^0.16.0",
    "composer-cli":"^0.16.0",
    "composer-client":"^0.16.0",
    "composer-common":"^0.16.0",
    "composer-connector-embedded":"^0.16.0",
    "eslint":"^3.6.1",
    "jsdoc":"^3.5.5",
    "license-check":"^1.1.5",
    "mkdirp":"^0.5.1",
    "mocha":"^3.2.0"
  },
  "license-check-config":{
    "src":["**/*.js","!./coverage/**/*","!./node_modules/**/*","!./out/**/*","!./scripts/**/*"],
    "path":"header.txt",
    "blocking":true,
    "logInfo":false,
    "logError":true
  },
  "dependencies":{
    "animaltracking-model":"^0.1.2",
    "body-parser": "^1.17.2",
    "cfenv": "~1.0.0",
    "composer-client": "^0.16.6",
    "config": "^1.26.1",
    "ejs": "^2.3.4",
    "express": "4.1.x",
    "multer": "^1.1.0"
  }
}